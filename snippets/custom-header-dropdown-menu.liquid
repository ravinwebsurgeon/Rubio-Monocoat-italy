                    {% endif %}
                  {% if block.settings.block_link != blank %}
                    <a href="{{- block.settings.block_link -}}"></a>
                  {% endif %}
                  </div>
                {% endif %}
              {% endif %}
            {% endfor %}
</div>
</ul>
</div>
{% unless link.title == "Products" %}
<div class="register-menu">
<a href="{{ section.settings.btn-link }}"><div class="reg-btn">{{ section.settings.btn }}</div></a>
</div>
{% endunless %}
      </div>
    {% else %}
