<div class="calc-overlay" id="calcOverlay"></div>

<div class="calc-panel" id="calcPanel">
  <button class="calc-close" id="closeCalc">    
    <svg width="30" height="30" viewBox="0 0 30 30" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
    <rect width="30" height="30" fill="url(#pattern0_794_672)"/>
    <defs>
    <pattern id="pattern0_794_672" patternContentUnits="objectBoundingBox" width="1" height="1">
    <use xlink:href="#image0_794_672" transform="scale(0.0078125)"/>
    </pattern>
    <image id="image0_794_672" width="128" height="128" preserveAspectRatio="none" xlink:href="data:image/png;base64,"/>
    </defs>
    </svg>
  </button>
  <div class="clac-box-img">    
    <svg width="30" height="30" viewBox="0 0 30 30" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
    <rect width="30" height="30" fill="url(#pattern0_794_671)"/>
    <defs>
    <pattern id="pattern0_794_671" patternContentUnits="objectBoundingBox" width="1" height="1">
    <use xlink:href="#image0_794_671" transform="scale(0.0078125)"/>
    </pattern>
    <image id="image0_794_671" width="128" height="128" preserveAspectRatio="none" xlink:href="data:image/png;base64,"/>
    </defs>
    </svg>
  </div>
  <h2>Try Our Paint Calculator</h2>
  <p>Our Paint Calculator will work out how much paint you need based on your room size.  
     We’ll give you an estimate for at least 2 coats.</p>

  <h3>Where are you painting?</h3>
  <div class="calc-choices">
    <button class="calc-choice" data-type="walls">Walls</button>
    <button class="calc-choice" data-type="ceiling">Ceiling</button>
  </div>

  <div class="calc-form" id="calcForm" style="display:none;">
    <label>Room Length (m):</label>
    <input type="number" id="length" placeholder="e.g. 5">

    <label>Room Width (m):</label>
    <input type="number" id="width" placeholder="e.g. 4">

    <label>Room Height (m):</label>
    <input type="number" id="height" placeholder="e.g. 2.5">

    <label>Coverage (m² per L):</label>
    <input type="number" id="coverage" placeholder="e.g. 12" value="12">

    <label>Number of Coats:</label>
    <input type="number" id="coats" placeholder="2" value="2">

    <button id="calcBtn">Calculate</button>
  </div>

  <div class="calc-result" id="calcResult"></div>
</div>

<style>

.calc-overlay {
  position:fixed;
  inset:0;
  background:rgba(0,0,0,0.4);
  display:none;
  z-index:999;
}
.calc-panel {
  position:fixed;
  top:0;
  right:-450px;
  max-width:400px;
  height:100%;
  background:#fff;
  box-shadow:-3px 0 10px rgba(0,0,0,0.2);
  padding:20px 20px 130px;
  z-index:1000;
  transition:right 0.3s ease;
  overflow-y:auto;
  font-family:sans-serif;  
}
.calc-panel h2 {
    color: #183029;
    font-size: 24px;
    line-height: normal;
    letter-spacing: normal;
    font-weight: 500;
    font-family: Frutiger Neue LT W1G;
    margin: 20px 0 10px;
}

.calc-panel p {
    margin-top: 0;
    color: #000;
    font-size: 14px;
    letter-spacing: normal;
    font-family: Frutiger Neue LT W1G;
    line-height: 1.5;
}

.calc-panel h3 {
    color: #183029;
    font-size: 24px;
    line-height: normal;
    letter-spacing: normal;
    font-weight: 500;
    font-family: Frutiger Neue LT W1G;
    margin: 30px 0 20px;
}
.calc-panel.open { 
    right:0; 
}
.clac-box-img svg {
    display: block;
}

.clac-box-img {
    margin-top: 27px;
}
.calc-close {
    position: absolute;
    top: 27px;
    right: 20px;
    background: none;
    cursor: pointer;
    padding: 0;
    border: none;
    outline: none;
}
.calc-close:focus {
    outline: none;
    box-shadow: none;
}
.calc-choices {
  display:flex;
  flex-direction:column;
  gap:10px;
  margin:20px 0;
}
.calc-choice {
  padding:12px;
  border:1px solid #183029;
  border-radius:25px;
  background:#fff;
  color:#183029;
  cursor:pointer;
  font-weight:300;
  font-family: Frutiger Neue LT W1G;
  line-height:20px;
  font-size:18px;
  letter-spacing:normal;
  font-family: Frutiger Neue LT W1G;
  text-transform:uppercase;
}
.calc-choice:focus{
 outline:none;
 box-shadow:none;
}
.calc-choice:hover { 
    background:#183029; 
    color:#fff; 
}
.calc-form label { 
    display:block; 
    margin:10px 0 5px; 
}
.calc-form input {
  width:100%; padding:8px; margin-bottom:10px;
  border:1px solid #ccc; border-radius:5px;
}
#calcBtn {
  display:block; width:100%; padding:12px;
  background:#183029; 
  color:#fff;
  border:none; border-radius:8px;
  cursor:pointer;
  font-weight:600;
}
.calc-result {
  margin-top:20px;
  padding:15px;
  background:#183029;
  border-radius:8px;
  font-weight:600;
  color:#fff;
}
</style>

<script>
document.addEventListener("DOMContentLoaded", function(){
  const openBtn   = document.getElementById("openCalc");
  const closeBtn  = document.getElementById("closeCalc");
  const panel     = document.getElementById("calcPanel");
  const overlay   = document.getElementById("calcOverlay");
  {% comment %} const choices   = document.querySelectorAll(".calc-choice");
  const form      = document.getElementById("calcForm");
  const calcBtn   = document.getElementById("calcBtn");
  const resultBox = document.getElementById("calcResult"); {% endcomment %}

  // Open
  openBtn.addEventListener("click", e=>{
    e.preventDefault();
    panel.classList.add("open");
    overlay.style.display="block";
  });

  // Close
  closeBtn.addEventListener("click", ()=>{
    panel.classList.remove("open");
    overlay.style.display="none";
    form.style.display="none";
    resultBox.innerHTML="";
  });
  overlay.addEventListener("click", ()=>closeBtn.click());

  // Choice
  choices.forEach(btn=>{
    btn.addEventListener("click", ()=>{
      form.style.display="block";
      resultBox.innerHTML="";
    });
  });

  // Calculate
  calcBtn.addEventListener("click", e=>{
    e.preventDefault();
    const length   = parseFloat(document.getElementById("length").value) || 0;
    const width    = parseFloat(document.getElementById("width").value) || 0;
    const height   = parseFloat(document.getElementById("height").value) || 0;
    const coverage = parseFloat(document.getElementById("coverage").value) || 1;
    const coats    = parseFloat(document.getElementById("coats").value) || 1;

    // Wall area (approx 2*(L+W)*H)
    const wallArea = 2 * (length + width) * height;

    // Ceiling area (L*W)
    const ceilingArea = length * width;

    // Total area
    const totalArea = wallArea + ceilingArea;

    const litersNeeded = (totalArea * coats) / coverage;
    const tins = Math.ceil(litersNeeded);

    resultBox.innerHTML = `
      <strong>Total Area:</strong> ${totalArea.toFixed(2)} m² <br>
      <strong>Liters Needed:</strong> ${litersNeeded.toFixed(1)} L <br>
      <strong>Suggested Tins:</strong> ${tins} x 1L tins
    `;
  });
});
</script>
