<div class="cust-project">
<div class="page-width">
<div class="cust-pro-wrap">
<h3 class="heading-pr">{{ section.settings.heading }}</h3>
<div class="main-pro-wrap">
<div class="pro-first">
<div class="Interio">
<img src="{{ section.settings.img1 | img_url:" " }}">
<div class="inter-btn" id="interior">Interior</div>
</div>
<div class="exterior">
    <img src="{{ section.settings.img2 | img_url:" " }}">
    <div class="exter-btn" id="exterior">Exterior</div>
</div>
    </div>
<div class="pro-second">

</div>
    </div>
    </div>
    </div>
    </div>
{% schema %}
{
  "name": "Project",
  "settings": [
    {
      "type": "text",
      "id": "heading",
      "label": "Main Heading",
      "default": "Choose your project:"
    },
    {
    "type": "image_picker",
    "id": "img1",
    "label": "Interior Image"
    },
     {
    "type": "image_picker",
    "id": "img2",
    "label": "Exterior Image"
    }
   
  ],
  
  "presets": [
    {
      "name": "Project",
      "category": "Custom"
    }
  ]
}
{% endschema %}

<style>
    .pro-f {
    display: flex;
    width: 100%;
    gap: 20;
    gap: 50px;
}

.Interio {
    width: 50%;
}

.exterior {
    width: 50%;
}

.Interio img {
    width: 100%;
}

.exterior img {
    width: 100%;
}
</style>

