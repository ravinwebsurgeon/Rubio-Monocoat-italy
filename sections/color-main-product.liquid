<script>
document.addEventListener('DOMContentLoaded', function () {
  const stickyForm = document.querySelector('.sticky-form');
  const stickyVariantIdEl = document.getElementById('sticky-variant-id');
  const stickyOptionsEl = document.getElementById('sticky-variant-options');
  const stickyPriceEl = document.getElementById('sticky-price');
  const stickyImgEl = document.getElementById('sticky-variant-image');
  const stickyBar = document.getElementById('sticky-add-to-cart');
  const mainAddToCartBtn = document.querySelector('.product-form__submit[name="add"]'); // main add-to-cart

  if (!stickyForm || !stickyBar) return;

  // ===== Sticky bar show/hide on scroll =====
  const productForm = document.querySelector('form[action="/cart/add"]');
  window.addEventListener('scroll', function () {
    const rect = productForm.getBoundingClientRect();
    stickyBar.style.display = rect.bottom < 0 ? 'flex' : 'none';
  });

  // ===== Get currently selected variant =====
  function getCurrentSelectedVariant() {
    const jsonEl = document.querySelector('[data-selected-variant]');
    if (!jsonEl) return null;
    try {
      return JSON.parse(jsonEl.textContent);
    } catch (e) {
      return null;
    }
  }

  // ===== Update sticky bar info =====
  function updateStickyBar() {
    const variant = getCurrentSelectedVariant();
    if (!variant) return;

    stickyOptionsEl.textContent = variant.options.filter(Boolean).join(' / ');

    if (typeof Shopify !== 'undefined' && Shopify.formatMoney) {
      stickyPriceEl.innerHTML = Shopify.formatMoney(
        variant.price,
        window.meta?.money_format || '{{ shop.money_format }}'
      );
    } else {
      stickyPriceEl.textContent = (variant.price / 100).toFixed(2);
    }

    stickyVariantIdEl.value = variant.id;

    if (variant.featured_image && stickyImgEl) {
      stickyImgEl.src = variant.featured_image.src;
    }
  }

  setInterval(updateStickyBar, 1000);
  updateStickyBar();

  // ===== Trigger main Add to Cart when sticky clicked =====
  stickyForm.addEventListener('submit', function (e) {
    e.preventDefault();

    // Update the main variant selection if needed
    const currentVariant = getCurrentSelectedVariant();
    if (currentVariant && stickyVariantIdEl.value != currentVariant.id) {
      stickyVariantIdEl.value = currentVariant.id;
    }

    // Trigger main Add to Cart button
    if (mainAddToCartBtn) {
      mainAddToCartBtn.click(); 
    } else {
      console.warn('Main Add to Cart button not found.');
    }
  });
});


document.addEventListener('DOMContentLoaded', () => {
  const productForm = document.querySelector('form[action="/cart/add"]');
  if (!productForm) return;

  const variantIdInput = productForm.querySelector('input[name="id"]');
  const seeMoreBtn = document.getElementById('customSeeMoreBtn');

  function showMoreContent() {
    const moreContentDivs = document.querySelectorAll('.SeeMoreContent');
    moreContentDivs.forEach(div => {
      div.style.setProperty('display', 'inline-block', 'important');
      div.style.visibility = 'visible';
      div.style.overflow = 'visible';
    });

    if (seeMoreBtn) {
      seeMoreBtn.style.display = 'none'; // hide the button after click
    }
  }

  if (seeMoreBtn) {
    seeMoreBtn.addEventListener('click', showMoreContent);
  }

  // Watch for variant changes
  let lastVariant = variantIdInput.value;
  const observer = new MutationObserver(() => {
    if (variantIdInput.value !== lastVariant) {
      lastVariant = variantIdInput.value;
      console.log('Variant changed:', lastVariant);
      
    if (window.Shopify && window.Shopify.product && window.Shopify.product.variants) {
      // Find the selected variant by ID
      const detailedVariant = window.Shopify.product.variants.find(variant => variant.id == variantIdInput.value);
      console.log(detailedVariant,'detailedVariant')
       if (detailedVariant) {
        const price = detailedVariant.price;
        const currency = Shopify.currency.active || 'USD';
        const currencyFormattedNumber = new Intl.NumberFormat('en-US', {
                style: 'currency',
                currency: currency
              }).format(price/100);
        let changePriceFor = document.querySelector('.custom_added_price')
        let oldPrice = changePriceFor.querySelector('.price-item.price-item--regular')
        oldPrice.textContent = `${currencyFormattedNumber}`;
       }
    }

      // If content was already expanded, reapply display
      const moreContentDivs = document.querySelectorAll('.SeeMoreContent');
      moreContentDivs.forEach(div => {
        if (seeMoreBtn.style.display === 'none') {
          div.style.setProperty('display', 'inline-block', 'important');
          div.style.visibility = 'visible';
          div.style.overflow = 'visible';
        }
      });
    }
  });

  observer.observe(variantIdInput, { attributes: true, attributeFilter: ['value'] });
  
  // Initial render: hide content initially
  const moreContentDivs = document.querySelectorAll('.SeeMoreContent');
  moreContentDivs.forEach(div => {
    div.style.setProperty('display', 'none', 'important');
    div.style.visibility = 'hidden';
    div.style.overflow = 'hidden';
  });

//select variant if redirection from colour page
  function getCookie(name) {
    const value = `; ${document.cookie}`;
    const parts = value.split(`; ${name}=`);
    if (parts.length === 2) return decodeURIComponent(parts.pop().split(';').shift());
  }

  function selectVariant(color)
  {
    console.log("Found variant from cookie:", color);
    let allOptions = document.querySelectorAll('.swatch-input__input');
    allOptions.forEach((option) => {
      if (option.value.trim().toLowerCase() === color.trim().toLowerCase()) {
        option.checked = true;
        option.dispatchEvent(new Event("change", { bubbles: true }));
      }
    });
  
  }
  const receivedData = getCookie("myvariant");
  if(receivedData) selectVariant(receivedData)
});

document.addEventListener('DOMContentLoaded', function () {

  setInterval(() => {
    const custom_btn = document.querySelector('.order_sample');
    if (custom_btn) {
      console.log('change btn');
      custom_btn.textContent = 'Order Sample';
    }
  }, 3000);
});

</script>




